{
  // 使用 IntelliSense 了解相关属性。
  // 悬停以查看现有属性的描述。
  // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/bot.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/tests",
      "justMyCode": false,
      "env": {
        // "LOG_LEVEL": "TRACE",
        "GITHUB_RUN_ID": "123",
        "GITHUB_EVENT_NAME": "issue_comment",
        "GITHUB_EVENT_PATH": "${workspaceFolder}/tests/events/issue-comment.json",
        "GITHUB_APPS": "[{\"client_id\": 1111, \"client_secret\": 12312}]",
        "GITHUB_REPOSITORY": "owner/repo",
        "INPUT_CONFIG": "{\"base\": \"master\", \"plugin_path\": \"website/static/plugins.json\", \"bot_path\": \"website/static/bots.json\", \"adapter_path\": \"website/static/adapters.json\"}",
        "APP_ID": "123",
        "PRIVATE_KEY": "123123"
      }
    },
    {
      "name": "Pytest",
      "type": "python",
      "request": "launch",
      "module": "pytest"
    },
    {
      "name": "Plugin Test",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/plugin_test.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "GITHUB_EVENT_NAME": "issue_comment",
        "GITHUB_EVENT_PATH": "${workspaceFolder}/tests/publish/events/issue-comment.json",
        "GITHUB_OUTPUT": "${workspaceFolder}/plugin_test/output.txt",
        "GITHUB_STEP_SUMMARY": "${workspaceFolder}/plugin_test/step_summary.txt",
      }
    },
    {
      "name": "Debug Unit Test",
      "type": "python",
      "request": "test",
      "justMyCode": false,
    },
  ]
}
